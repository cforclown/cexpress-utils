{
  "name": "cexpress-utils",
  "version": "1.6.3",
  "description": "Base utils for ExpressJS framework",
  "author": "Hafis Alrizal",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/cforclown/cexpress-utils"
  },
  "homepage": "https://github.com/cforclown/cexpress-utils#readme",
  "bugs": {
    "url": "https://github.com/cforclown/cexpress-utils/issues"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Base",
    "Utils"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:check": "npm run lint",
    "lint:format": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "prepare": "npm run build",
    "prepublishOnly ": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "nanoid": "^5.0.4",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "jest": "^29.7.0",
    "jest-mock-req-res": "^1.0.2",
    "lint-staged": "^13.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "npm run lint-check"
    ]
  },
  "volta": {
    "node": "20.10.0"
  }
}
